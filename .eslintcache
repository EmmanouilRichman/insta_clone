Submit}>\n                   <div className=\"form-group\">\n                       <label>Title: </label>\n                       <input type=\"text\" \n                       className=\"form-control\" \n                       value={this.state.title} \n                       onChange={this.onChangeTitle} />\n                    </div>\n                    <div className=\"form-group\">\n                        <label>Description: </label>\n                        <input type=\"text\"\n                        className=\"form-control\"\n                        value={this.state.description}\n                        onChange={this.onChangeDescription} />\n                    </div>\n                    <div className=\"form-group\">\n                        <label>Photo Link: </label>\n                        <input type=\"text\"\n                        className=\"form-control\"\n                        value={this.state.img}\n                        onChange={this.onChangeImg}/>\n                    </div>\n                    <div className=\"form-group\">\n                        <input type=\"submit\" value=\"Edit Post\" className=\"btn btn-primary\" />\n                    </div>\n               </form>\n            </div>\n        )\n    }\n}","/Users/manny/insta-clone/src/Components/Navbar.jsx",[],"/Users/manny/insta-clone/src/Components/CreatePost.jsx",[],"/Users/manny/insta-clone/src/Components/PostList.jsx",["53"],"/Users/manny/insta-clone/src/Components/Login.js",[],"/Users/manny/insta-clone/src/Components/Logout.js",[],"/Users/manny/insta-clone/src/Components/NavItems.js",[],"/Users/manny/insta-clone/src/Components/Profile.js",[],"/Users/manny/insta-clone/src/Components/ChatRoom.js",[],{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","severity":1,"message":"59","line":3,"column":9,"nodeType":"60","messageId":"61","endLine":3,"endColumn":14},{"ruleId":"58","severity":1,"message":"62","line":3,"column":10,"nodeType":"60